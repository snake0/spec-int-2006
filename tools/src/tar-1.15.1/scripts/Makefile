	done

installcheck-sbinSCRIPTS: $(sbin_SCRIPTS)
	bad=0; pid=$$$$; list="$(sbin_SCRIPTS)"; for p in $$list; do \
	  case ' $(AM_INSTALLCHECK_STD_OPTIONS_EXEMPT) ' in \
	   *" $$p "* | *" $(srcdir)/$$p "*) continue;; \
